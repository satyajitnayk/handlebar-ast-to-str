{
	"name": "hbs-ast-to-str",
	"version": "1.0.4",
	"description": "Converts Handlebars Abstract Syntax Tree (AST) back to template string with options for processing(optional).",
	"main": "dist/cjs/index.js",
	"files": [
		"dist"
	],
	"scripts": {
		"test": "jest",
		"build": "npm run clean:dist && npm run build:esm && npm run build:cjs && npm run build:web",
		"clean:dist": "rm -rf dist/",
		"build:esm": "tsc",
		"build:cjs": "tsc --module CommonJS --outDir dist/cjs",
		"build:web": "npx webpack --mode=production"
	},
	"author": "satyajit nayak",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/satyajitnayk/handlebar-ast-to-str"
	},
	"dependencies": {
		"handlebars": "^4.7.8"
	},
	"devDependencies": {
		"@types/jest": "^29.5.6",
		"jest": "^29.7.0",
		"ts-jest": "^29.1.1",
		"ts-loader": "^9.5.0",
		"typescript": "^5.2.2",
		"webpack": "^5.89.0",
		"webpack-cli": "^5.1.4"
	},
	"module": "commonjs",
	"browser": {
		"fs": false,
		"path": false,
		"os": false
	}
}
